    "Optimizely",
    "Browserify",
    "ES6",
    "JSX"
  ],
  "author": "casecode <casey.r.white@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clearhead/gulp-clearbuild/issues"
  },
  "homepage": "https://github.com/clearhead/gulp-clearbuild#readme",
  "dependencies": {
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "comment-regex": "^1.0.0",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-csslint": "^0.1.5",
    "gulp-eslint": "^1.0.0",
    "gulp-help": "^1.6.0",
    "gulp-sass": "^2.0.4",
    "gulp-sequence": "^0.4.0",
    "gulp-shell": "^0.4.2",
    "process-finder": "^1.0.0",
